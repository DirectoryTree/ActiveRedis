{
    "name": "directorytree/activeredis",
    "description": "An ActiveRecord implementation for Redis.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DirectoryTree\\ActiveRedis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DirectoryTree\\ActiveRedis\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DirectoryTree\\ActiveRedis\\ActiveRedisServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "illuminate/collections": "^9.0|^10.0|^11.0"
    },
    "suggest" : {
        "brick/math": "For decimal cast support"
    },
    "require-dev": {
        "predis/predis": "^2.0",
        "laravel/pint": "^1.17",
        "pestphp/pest": "^1.0|^2.0|^3.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
